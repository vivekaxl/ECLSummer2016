NAME dedup_all_test;
LABEL "dedup_all_test";
VERSION "1.0.0";
AUTHOR "NairVi01";
CATEGORY CLEAN;
DESCRIPTION "Sample Plugin Template. This template takes an input dataset and appends three fields.
 the appended dataset is exposed for downstream plugins to hook into.";

PERMISSIONS
   VIEW:PUBLIC;
   EDIT:PRIVATE;
   RUN:PUBLIC;
END

INPUTS
        DATASET dsInput
        END
END

OUTPUTS
        DATASET dsOutput(dsInput)
        END
END


 GENERATES INLINE
%^edsOutput% := DEDUP(SORT(%dsInput%, RECORD), RECORD);
ENDGENERATES